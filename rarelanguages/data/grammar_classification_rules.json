    "irregular_verbs": ["jam", "kam", "shkoj", "vij", "ha", "pi", "them", "jap", "marr"],
    "modal_verbs": ["dua", "mund", "duhet", "guxoj"],
    "auxiliary_verbs": ["jam", "kam", "do"]
