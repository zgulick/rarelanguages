  "albanian_gender_rules": {
    "masculine_endings": ["i", "u", "ë"],
    "feminine_endings": ["a", "e"],
    "masculine_indicators": ["i mirë", "i madh", "i ri", "i vjetër"],
    "feminine_indicators": ["e mirë", "e madhe", "e re", "e vjetër"],
    "irregular_genders": {
      "nënë": "feminine",
      "atë": "masculine",
      "vëlla": "masculine",
      "motër": "feminine",
      "fëmij": "neuter",
      "sy": "masculine",
      "dorë": "feminine",
      "kokë": "feminine",
      "trup": "masculine"
